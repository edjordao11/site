{
  "name": "videosplus",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "server": "node server.js",
    "dev:all": "concurrently \"npm run dev\" \"npm run server\"",
    "build": "vite build && cp public/_redirects dist/ && cp public/_headers dist/ && cp netlify.headers dist/_headers || true && cp static.json dist/ || true",
    "build:typecheck": "tsc && vite build && cp public/_redirects dist/ && cp public/_headers dist/ && cp netlify.headers dist/_headers || true && cp static.json dist/ || true",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "start": "NODE_ENV=production node server.js"
  },
  "dependencies": {
    "@emotion/react": "^11.11.3",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.15.10",
    "@mui/material": "^5.15.10",
    "@paypal/react-paypal-js": "^8.8.3",
    "@stripe/react-stripe-js": "^3.7.0",
    "@stripe/stripe-js": "^7.4.0",
    "@vercel/analytics": "^1.5.0",
    "@videojs/themes": "^1.0.1",
    "appwrite": "^13.0.1",
    "body-parser": "^1.20.3",
    "cors": "^2.8.5",
    "dotenv": "^16.6.1",
    "express": "^4.21.2",
    "jspdf": "^3.0.1",
    "node-appwrite": "^17.0.0",
    "nodemailer": "^6.10.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.22.1",
    "sass": "^1.71.0",
    "stripe": "^12.18.0",
    "video.js": "^8.22.0"
  },
  "devDependencies": {
    "@types/node": "^24.0.5",
    "@types/react": "^18.2.55",
    "@types/react-dom": "^18.2.19",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@vitejs/plugin-react": "^4.2.1",
    "concurrently": "^8.2.2",
    "eslint": "^8.56.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "nodemon": "^3.1.10",
    "typescript": "^5.2.2",
    "vite": "^5.1.0"
  }
}
